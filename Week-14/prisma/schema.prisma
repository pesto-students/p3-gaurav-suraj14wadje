generator client {
  provider        = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URI")
}

model User{
    id        Int       @id @default(autoincrement())
    name      String        @db.VarChar(100)
    email     String    @unique @db.VarChar(100)
    password  String?   @db.VarChar(200)
}